@model AdminPanel1.DbContext.User
@{
    ViewData["Title"] = "Mening Profilim";
}

<h2>Mening Profilim</h2>

@if (TempData["Success"] != null)
{
    <div class="alert alert-success">@TempData["Success"]</div>
}

<div class="mb-3"><strong>Full Name:</strong> @Model.FullName</div>
<div class="mb-3"><strong>Username:</strong> @Model.UserName</div>
<div class="mb-3"><strong>Email:</strong> @Model.Email</div>

<a asp-action="Edit" class="btn btn-primary">Tahrirlash</a>
<a asp-action="ChangePassword" class="btn btn-warning">Parolni o'zgartirish</a>
